{
  "name": "yet_another_rss_bot",
  "description": "Telegram Bot for forwarding webhooks",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "Samuel Gratzl",
    "email": "sam@sgratzl.com",
    "url": "https://www.sgratzl.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sgratzl/yet_another_rss_bot.git"
  },
  "bugs": {
    "url": "https://github.com/sgratzl/yet_another_rss_bot/issues"
  },
  "homepage": "https://github.com/sgratzl/yet_another_rss_bot#readme",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "compile": "tsc",
    "watch": "tsc --watch",
    "postbuild": "npm run test",
    "build": "npm run compile",
    "test": "eslint -c ./.eslintrc.js 'api/**/*.ts'",
    "release:major": "release-it major",
    "release:minor": "release-it minor",
    "release:patch": "release-it patch",
    "release:pre": "release-it --preRelease=alpha --npm.tag=next",
    "start": "ts-node -r dotenv/config ./api/bot.ts",
    "webhook": "ts-node -r dotenv/config ./api/bot.ts https://yetanotherrssbot.sgratzl.now.sh/api/bot"
  },
  "dependencies": {
    "@types/feedparser": "^2.2.3",
    "@types/mongodb": "^3.5.22",
    "@types/node": "^14.0.13",
    "@types/node-fetch": "^2.5.7",
    "@types/uuid": "^8.0.0",
    "feedparser": "^2.2.10",
    "mongodb": "^3.5.9",
    "node-fetch": "^2.6.0",
    "telegraf": "^3.38.0",
    "telegraf-inline-menu": "^4.0.1",
    "telegram-typings": "^4.0.0-beta",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@now/node": "^1.7.1",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "@yarnpkg/pnpify": "^2.0.0-rc.25",
    "dotenv": "^8.2.0",
    "eslint": "^7.2.0",
    "now": "^19.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
