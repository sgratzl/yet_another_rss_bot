{
  "name": "yet_another_rss_bot",
  "description": "Telegram Bot for forwarding webhooks",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "Samuel Gratzl",
    "email": "sam@sgratzl.com",
    "url": "https://www.sgratzl.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sgratzl/yet_another_rss_bot.git"
  },
  "bugs": {
    "url": "https://github.com/sgratzl/yet_another_rss_bot/issues"
  },
  "homepage": "https://github.com/sgratzl/yet_another_rss_bot#readme",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "compile": "tsc",
    "watch": "tsc --watch",
    "postbuild": "npm run test",
    "build": "npm run compile",
    "test": "eslint -c ./.eslintrc.js 'api/**/*.ts'",
    "release:major": "release-it major",
    "release:minor": "release-it minor",
    "release:patch": "release-it patch",
    "release:pre": "release-it --preRelease=alpha --npm.tag=next",
    "start": "ts-node -r dotenv/config ./api/bot.ts",
    "webhook": "ts-node -r dotenv/config ./api/bot.ts https://yetanotherrssbot.sgratzl.now.sh/api/bot"
  },
  "dependencies": {
    "@types/feedparser": "^2.2.3",
    "@types/mongodb": "^3.6.8",
    "@types/node": "^14.14.31",
    "@types/node-fetch": "^2.5.8",
    "@types/uuid": "^8.3.0",
    "feedparser": "^2.2.10",
    "mongodb": "^3.6.4",
    "node-fetch": "^2.6.1",
    "telegraf": "^4.1.0",
    "telegraf-inline-menu": "^6.2.0",
    "telegram-typings": "^5.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@now/node": "^1.7.1",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "@yarnpkg/pnpify": "^2.4.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.20.0",
    "now": "^19.1.1",
    "ts-node": "^9.1.1",
    "typescript": "~4.1.0"
  }
}
